#include <stdio.h>

int main()
{
    int n,r,r2,rv=0;
    scanf("%d",&n);
    while(n!=0)
    {
        r=n%10;
        rv=rv*10+r;
        n=n/10;
        
    }
    while(rv!=0)
    {
        r2=rv%10;
        switch (r2)
        {
            case 1:
                printf("one ");
                break;
            case 2:
                printf("two ");
                break;
            case 3:
                printf("three ");
                break;
            case 4:
                printf("four ");
                break;
            case 5:
                printf("five ");
                break;
            case 6:
                printf("six ");
                break;
            case 7:
                printf("seven ");
                break;
            case 8:
                printf("eight ");
                break;
            case 9:
                printf("nine ");
                break;
            case 0:
                printf("zero ");
                break;    
        }
        rv=rv/10;
    }
}
